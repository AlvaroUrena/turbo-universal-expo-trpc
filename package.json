{
  "name": "turbo-universal-expo-trpc",
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "turbo": "^2.1.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.18.2"
  },
  "packageManager": "bun@1.1.29",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "rm -rf node_modules",
    "clean:workspaces": "turbo clean",
    "dev": "turbo dev",
    "format": "turbo format --continue --",
    "format:fix": "turbo format:fix --continue --",
    "lint": "turbo lint --continue --",
    "lint:fix": "turbo lint:fix --continue --",
    "deploy": "turbo deploy"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
